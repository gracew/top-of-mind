{
  "name": "Top of Mind",
  "description": "Automatically generate comments on Linkedin posts to keep you top of mind in your network.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [],
  "content_scripts": [{
    "matches": ["*://*.linkedin.com/*"],
    "js": ["contentScript.js"]
  }],
  "action": {
    "default_popup": "./index.html",
    "default_icon": {
      "16": "logo16.png",
      "32": "logo32.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}
